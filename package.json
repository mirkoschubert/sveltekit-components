{
  "name": "sveltekit-components",
  "version": "0.4.0",
  "description": "An opinionated UI library of SvelteKit components",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "update": "npm update @sveltejs/kit && npx npm-check -u",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0-next.64",
    "@sveltejs/adapter-vercel": "^1.0.0-next.66",
    "@sveltejs/kit": "^1.0.0-next.401",
    "autoprefixer": "^10.4.8",
    "clipboard-copy": "^4.0.1",
    "cssnano": "^5.1.5",
    "cssnano-preset-default": "^5.2.5",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "prism-svelte": "^0.5.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.27.0",
    "remark-slug": "^7.0.1",
    "sass": "^1.54.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-feather-icons": "^4.0.0",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.13",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "unist-util-visit": "^4.1.0",
    "vite": "^3.0.4"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.6",
    "@fontsource/source-sans-pro": "^4.5.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mirkoschubert/sveltekit-components.git"
  },
  "homepage": "https://sveltekit-components.mirkoschubert.com",
  "bugs": "https://github.com/mirkoschubert/sveltekit-components/issues",
  "keywords": [
    "svelte",
    "sveltekit",
    "component library",
    "ui library",
    "ui kit"
  ]
}
