{
  "name": "sveltekit-components",
  "version": "0.1.0",
  "description": "An opinionated UI library of SvelteKit components",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-vercel": "^1.0.0-next.47",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.4.4",
    "clipboard-copy": "^4.0.1",
    "cssnano": "^5.1.5",
    "cssnano-preset-default": "^5.2.5",
    "mdsvex": "^0.10.5",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.3",
    "postcss-uncss": "^0.17.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prism-svelte": "^0.5.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.27.0",
    "remark-slug": "^7.0.1",
    "sass": "^1.49.9",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-feather-icons": "^3.5.1",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.6",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "unist-util-visit": "^4.1.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.6",
    "@fontsource/source-sans-pro": "^4.5.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mirkoschubert/sveltekit-components.git"
  },
  "homepage": "https://sveltekit-components.mirkoschubert.com",
  "bugs": "https://github.com/mirkoschubert/sveltekit-components/issues",
  "keywords": [
    "svelte",
    "sveltekit",
    "component library",
    "ui library",
    "ui kit"
  ]
}
